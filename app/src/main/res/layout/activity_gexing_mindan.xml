<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/gexing_mingdan_imageview"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:scaleType="centerCrop"
        android:layout_marginStart="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>
    <TextView
        android:id="@+id/gexing_mingdan_textview"
        android:layout_width="200dp"
        android:layout_height="35dp"
        android:text="liuxing"
        android:textColor="@color/white"
        android:textSize="20dp"
        android:layout_marginStart="10dp"
        app:layout_constraintStart_toEndOf="@id/gexing_mingdan_imageview"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/gexing_mingdan_textview"
        app:layout_constraintTop_toTopOf="@id/gexing_mingdan_textview"
        app:layout_constraintBottom_toBottomOf="@id/gexing_mingdan_textview"
        android:text=">"
        android:textSize="30sp"
        android:textColor="@color/white"/>


</androidx.constraintlayout.widget.ConstraintLayout>